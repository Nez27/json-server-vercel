{
  "functions": {
    "api/main.py": {
      "runtime": "vercel/python@4.1.0"
    },
    "api/server.js": {
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/api/express/(.*)",
      "destination": "api/server.js"
    },
    {
      "source": "/api/flask/(.*)",
      "destination": "api/main.py"
    }
  ]
}
